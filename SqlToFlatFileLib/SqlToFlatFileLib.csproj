<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>SqlToFlatFileLib</AssemblyName>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <Version>1.0.4</Version>
    <AssemblyTitle>SqlToFlatFileLib</AssemblyTitle>
    <Description>Dump any data source (ODBC, OleDB, Sql Native) to a delimited file.</Description>
    <Copyright>2018</Copyright>
    <PackageProjectUrl>https://github.com/LarrySmith-1437/SqlToFlatFile</PackageProjectUrl>
    <RepositoryUrl>https://github.com/LarrySmith-1437/SqlToFlatFile</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>Export Flat File</PackageTags>
    <Authors>Lawrence M. Smith</Authors>
    <PackageReleaseNotes>Downgraded the .net build target to 4.5.2 (from 4.6.2) to allow lower frameworks to be able to use it.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.8" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
</Project>  